{
  "name": "@copilotkit/menu-analysis",
  "version": "1.0.0",
  "description": "Automated menu functionality analysis using Playwright and LLM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "analyze": "node dist/cli.js"
  },
  "keywords": [
    "menu-analysis",
    "playwright",
    "llm",
    "web-crawler",
    "automation"
  ],
  "author": "CopilotKit",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.2.0",
    "openai": "^4.20.0",
    "playwright": "^1.40.0",
    "winston": "^3.11.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
